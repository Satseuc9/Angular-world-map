{
    "version": 2,
    "builds": [
      {
        "src": "angular.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/angular-world-map"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/dist/angular-world-map/index.html" }
    ]
  }
  